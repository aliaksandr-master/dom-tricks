{
  "name": "dom-tricks",
  "version": "1.0.0",
  "description": "redux api middleware for separate your api routes, api actions, and interceptors. provide state normalization, db reducer",
  "main": "lib/index.js",
  "scripts": {
    "cleanup": "npm prune && npm i && npm dedupe",
    "test": "node_modules/.bin/eslint --color --debug --config ./.eslintrc.json *.js lib/**/*.js lib/*.js && node_modules/.bin/jest --coverage",
    "jest": "node_modules/.bin/jest --coverage",
    "precommit": "npm test",
    "prepush": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aliaksandr-master/dom-tricks.git"
  },
  "keywords": [
    "dom",
    "tricks",
    "library"
  ],
  "author": "alxe.master@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aliaksandr-master/dom-tricks/issues"
  },
  "homepage": "https://github.com/aliaksandr-master/dom-tricks#readme",
  "devDependencies": {
    "babel-core": "6.21.0",
    "babel-eslint": "7.1.1",
    "babel-jest": "18.0.0",
    "babel-loader": "6.2.10",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-stage-2": "6.18.0",
    "babel-runtime": "6.20.0",
    "eslint": "3.12.2",
    "eslint-config-eslint": "3.0.0",
    "eslint-import-resolver-webpack": "0.8.0",
    "eslint-loader": "1.6.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-lodash": "^2.4.5",
    "husky": "0.12.0",
    "jest": "18.1.0",
    "jest-cli": "18.1.0",
    "lodash": "4.17.4",
    "webpack": "1.14.0"
  },
  "peerDependencies": {
    "lodash": "^4.17.4"
  },
  "dependencies": {},
  "jest": {
    "automock": false,
    "notify": true,
    "testEnvironment": "jsdom",
    "cacheDirectory": ".tmp/jest-cache",
    "coverageDirectory": ".tmp/tests-coverage-report",
    "collectCoverageFrom": [
      "**/*.{js,jsx}"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/"
    ],
    "moduleFileExtensions": [
      "",
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "verbose": false
  }
}
